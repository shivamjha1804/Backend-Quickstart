{
  "name": "create-backend-quickstart",
  "version": "1.0.0",
  "description": "A comprehensive, production-ready backend boilerplate generator with Express.js, TypeScript, Sequelize, JWT auth, and more",
  "main": "dist/index.js",
  "bin": {
    "create-backend-quickstart": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint"
  },
  "keywords": [
    "backend",
    "boilerplate",
    "express",
    "typescript",
    "sequelize",
    "jwt",
    "auth",
    "api",
    "rest",
    "generator",
    "cli",
    "scaffold",
    "quickstart"
  ],
  "author": "Backend Quickstart Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/backend-quickstart/create-backend-quickstart.git"
  },
  "bugs": {
    "url": "https://github.com/backend-quickstart/create-backend-quickstart/issues"
  },
  "homepage": "https://github.com/backend-quickstart/create-backend-quickstart#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "inquirer": "^8.2.6",
    "fs-extra": "^11.1.1",
    "ora": "^5.4.1",
    "validate-npm-package-name": "^5.0.0",
    "semver": "^7.5.4",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/inquirer": "^9.0.3",
    "@types/fs-extra": "^11.0.1",
    "@types/mustache": "^4.2.2",
    "@types/semver": "^7.5.0",
    "@types/validate-npm-package-name": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "@types/jest": "^29.5.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "prettier": "^3.0.2"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ]
}